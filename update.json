---

{
    "latest_version": "{{site.release_version}}",
    "releases": [
        {
            "platform": "win32",
            "portable": false,
            "release_url": "",
            "direct_url": ""
        },
        {
            "platform": "win32",
            "portable": true,
            "release_url": "",
            "direct_url": ""
        }
    ],
    "changelog": [{% for release in site.changelog %}{% if release.version %} 
        {
            "version": "{{ release.version }}",{% endif %}{% if release.changes %}
            "changes": [{% for change in release.changes %}
                "{{ change.change }}"{% if change != release.changes.last %},{% endif %}{% endfor %}
            ]
        }{% if release != site.changelog.last %},{% endif %}{% endif %}{% endfor %}
    
    ]
}